        <TextBox HorizontalAlignment="Left" Height="23" Margin="101,87,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Label Content="Gender" HorizontalAlignment="Left" Margin="38,118,0,0" VerticalAlignment="Top"/>
